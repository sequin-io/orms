model event {
  id                       String    @id
  account_id               String?
  api_version              String?
  created                  DateTime? @db.Timestamp(6)
  data_object              Json?
  data_previous_attributes Json?
  livemode                 Boolean?
  object                   String?
  pending_webhooks         BigInt?
  type                     String?
  account                  account?  @relation(fields: [account_id], references: [id])

  @@map("event")
}

model fee_refund {
  id                     String               @id
  amount                 BigInt?
  balance_transaction_id String?
  created                DateTime?            @db.Timestamp(6)
  currency               String?
  fee_id                 String?
  metadata               Json?
  object                 String?
  balance_transaction    balance_transaction? @relation(fields: [balance_transaction_id], references: [id])
  application_fee        application_fee?     @relation(fields: [fee_id], references: [id])

  @@index([balance_transaction_id], name: "fee_refund_balance_transaction_id_idx")
  @@index([fee_id], name: "fee_refund_fee_id_idx")
  @@map("fee_refund")
}

model file {
  id                                                               String                 @id
  created                                                          DateTime?              @db.Timestamp(6)
  expires_at                                                       DateTime?              @db.Timestamp(6)
  filename                                                         String?
  object                                                           String?
  purpose                                                          String?
  size                                                             BigInt?
  title                                                            String?
  type                                                             String?
  url                                                              String?
  dispute_dispute_evidence_cancellation_policy_idTofile            dispute[]              @relation("dispute_evidence_cancellation_policy_idTofile")
  dispute_dispute_evidence_customer_communication_idTofile         dispute[]              @relation("dispute_evidence_customer_communication_idTofile")
  dispute_dispute_evidence_customer_signature_idTofile             dispute[]              @relation("dispute_evidence_customer_signature_idTofile")
  dispute_dispute_evidence_duplicate_charge_documentation_idTofile dispute[]              @relation("dispute_evidence_duplicate_charge_documentation_idTofile")
  dispute_dispute_evidence_receipt_idTofile                        dispute[]              @relation("dispute_evidence_receipt_idTofile")
  dispute_dispute_evidence_refund_policy_idTofile                  dispute[]              @relation("dispute_evidence_refund_policy_idTofile")
  dispute_dispute_evidence_service_documentation_idTofile          dispute[]              @relation("dispute_evidence_service_documentation_idTofile")
  dispute_dispute_evidence_shipping_documentation_idTofile         dispute[]              @relation("dispute_evidence_shipping_documentation_idTofile")
  evidence_uncategorized_disputes                                  dispute[]              @relation("dispute_evidence_uncategorized_file_idTofile")
  file_links                                                       file_link[]
  reporting_report_runs                                            reporting_report_run[]
  scheduled_query_runs                                             scheduled_query_run[]

  @@map("file")
}

model file_link {
  id         String    @id
  created    DateTime? @db.Timestamp(6)
  expired    Boolean?
  expires_at DateTime? @db.Timestamp(6)
  file_id    String?
  livemode   Boolean?
  metadata   Json?
  object     String?
  url        String?
  file       file?     @relation(fields: [file_id], references: [id])

  @@index([file_id], name: "file_link_file_id_idx")
