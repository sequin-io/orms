  evidence_cancellation_policy               file?           @relation("dispute_evidence_cancellation_policy_idTofile", fields: [evidence_cancellation_policy_id], references: [id])
  evidence_customer_communication            file?           @relation("dispute_evidence_customer_communication_idTofile", fields: [evidence_customer_communication_id], references: [id])
