  @@index([charge_id], name: "dispute_charge_id_idx")
  @@index([evidence_cancellation_policy_id], name: "dispute_evidence_cancellation_policy_id_idx")
  @@index([evidence_customer_communication_id], name: "dispute_evidence_customer_communication_id_idx")
  @@index([evidence_customer_signature_id], name: "dispute_evidence_customer_signature_id_idx")
  @@index([evidence_duplicate_charge_documentation_id], name: "dispute_evidence_duplicate_charge_documentation_id_idx")
  @@index([evidence_receipt_id], name: "dispute_evidence_receipt_id_idx")
  @@index([evidence_refund_policy_id], name: "dispute_evidence_refund_policy_id_idx")
  @@index([evidence_service_documentation_id], name: "dispute_evidence_service_documentation_id_idx")
  @@index([evidence_shipping_documentation_id], name: "dispute_evidence_shipping_documentation_id_idx")
  @@index([evidence_uncategorized_file_id], name: "dispute_evidence_uncategorized_file_id_idx")
