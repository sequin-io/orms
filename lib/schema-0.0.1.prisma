model usage_record_summary {
  id                   String             @id
  invoice_id           String?
  livemode             Boolean?
  object               String?
  period_end           DateTime?          @db.Timestamp(6)
  period_start         DateTime?          @db.Timestamp(6)
  subscription_item_id String?
  total_usage          BigInt?
  invoice              invoice?           @relation(fields: [invoice_id], references: [id])
  subscription_item    subscription_item? @relation(fields: [subscription_item_id], references: [id])

  @@map("usage_record_summary")
}

model webhook_endpoint {
  id             String       @id
  api_version    String?
  application_id String?
  created        DateTime?    @db.Timestamp(6)
  deleted        Boolean?
  description    String?
  enabled_events String[]
  livemode       Boolean?
  metadata       Json?
  object         String?
  secret         String?
  status         String?
  url            String?
  application    application? @relation(fields: [application_id], references: [id])

  @@map("webhook_endpoint")
}
