model customer_balance_transaction {
  id             String        @id
  amount         BigInt?
  created        DateTime?     @db.Timestamp(6)
  credit_note_id String?
  currency       String?
  customer_id    String?
  description    String?
  ending_balance BigInt?
  invoice_id     String?
  livemode       Boolean?
  metadata       Json?
  object         String?
  type           String?
  credit_note    credit_note?  @relation("credit_noteTocustomer_balance_transaction_credit_note_id", fields: [credit_note_id], references: [id])
  customer       customer?     @relation(fields: [customer_id], references: [id])
  invoice        invoice?      @relation(fields: [invoice_id], references: [id])
  credit_notes   credit_note[] @relation("credit_note_customer_balance_transaction_idTocustomer_balance_transaction")

  @@index([credit_note_id], name: "customer_balance_transaction_credit_note_id_idx")
  @@index([customer_id], name: "customer_balance_transaction_customer_id_idx")
  @@index([invoice_id], name: "customer_balance_transaction_invoice_id_idx")
  @@map("customer_balance_transaction")
}

model discount {
  id                                                    String                          @id
  checkout_session                                      String?
  coupon_id                                             String?
  customer_id                                           String?
  deleted                                               Boolean?
  end                                                   DateTime?                       @db.Timestamp(6)
  invoice_id                                            String?
  invoice_item                                          String?
  object                                                String?
  promotion_code_id                                     String?
  start                                                 DateTime?                       @db.Timestamp(6)
  subscription_id                                       String?
  coupon                                                coupon?                         @relation(fields: [coupon_id], references: [id])
  customer                                              customer?                       @relation("customerTodiscount_customer_id", fields: [customer_id], references: [id])
  invoice                                               invoice?                        @relation("discount_invoice_idToinvoice", fields: [invoice_id], references: [id])
  promotion_code                                        promotion_code?                 @relation(fields: [promotion_code_id], references: [id])
  subscription                                          subscription?                   @relation("discount_subscription_idTosubscription", fields: [subscription_id], references: [id])
  credit_notes                                          credit_note[]
  credit_note_line_items                                credit_note_line_item[]
  customers                                             customer[]                      @relation("customer_discount_idTodiscount")
  invoices                                              invoice[]                       @relation("discountToinvoice_discount_id")
  total_discount_amounts_invoices                       invoice[]                       @relation("discountToinvoice_total_discount_amounts_discount_id")
  items                                                 item[]
  line_items                                            line_item[]
  subscriptions                                         subscription[]                  @relation("discountTosubscription_discount_id")
  upcoming_customer_invoices                            upcoming_customer_invoice[]     @relation("discountToupcoming_customer_invoice_discount_id")
  total_discount_amounts_upcoming_customer_invoices     upcoming_customer_invoice[]     @relation("discountToupcoming_customer_invoice_total_discount_amounts_discount_id")
  upcoming_line_items                                   upcoming_line_item[]
  upcoming_subscription_invoices                        upcoming_subscription_invoice[] @relation("discountToupcoming_subscription_invoice_discount_id")
  total_discount_amounts_upcoming_subscription_invoices upcoming_subscription_invoice[] @relation("discountToupcoming_subscription_invoice_total_discount_amounts_discount_id")

  @@index([coupon_id], name: "discount_coupon_id_idx")
  @@index([customer_id], name: "discount_customer_id_idx")
  @@index([promotion_code_id], name: "discount_promotion_code_id_idx")
  @@map("discount")
}

