model coupon {
  id                     String                  @id
  amount_off             BigInt?
  applies_to_products    String[]
  created                DateTime?               @db.Timestamp(6)
  currency               String?
  deleted                Boolean?
  duration               String?
  duration_in_months     BigInt?
  livemode               Boolean?
  max_redemptions        BigInt?
  metadata               Json?
  name                   String?
  object                 String?
  percent_off            Decimal?                @db.Decimal
  redeem_by              DateTime?               @db.Timestamp(6)
  times_redeemed         BigInt?
  valid                  Boolean?
  discounts              discount[]
  promotion_codes        promotion_code[]
  subscription_schedules subscription_schedule[]

  @@map("coupon")
}

model credit_note {
  id                              String                         @id
  amount                          BigInt?
  created                         DateTime?                      @db.Timestamp(6)
  currency                        String?
  customer_balance_transaction_id String?
  customer_id                     String?
  discount_amount                 BigInt?
  discount_amounts_amount         BigInt?
  discount_amounts_discount_id    String?
  invoice_id                      String?
  livemode                        Boolean?
  memo                            String?
  metadata                        Json?
  number                          String?
  object                          String?
  out_of_band_amount              BigInt?
  pdf                             String?
  reason                          String?
  refund_id                       String?
  status                          String?
  subtotal                        BigInt?
  tax_amounts_amount              BigInt?
  tax_amounts_inclusive           Boolean?
  tax_amounts_tax_rate_id         String?
  total                           BigInt?
  type                            String?
  voided_at                       DateTime?                      @db.Timestamp(6)
  customer_balance_transaction    customer_balance_transaction?  @relation("credit_note_customer_balance_transaction_idTocustomer_balance_transaction", fields: [customer_balance_transaction_id], references: [id])
  customer                        customer?                      @relation(fields: [customer_id], references: [id])
  discount                        discount?                      @relation(fields: [discount_amounts_discount_id], references: [id])
  invoice                         invoice?                       @relation(fields: [invoice_id], references: [id])
  refund                          refund?                        @relation(fields: [refund_id], references: [id])
  tax_rate                        tax_rate?                      @relation(fields: [tax_amounts_tax_rate_id], references: [id])
  customer_balance_transactions   customer_balance_transaction[] @relation("credit_noteTocustomer_balance_transaction_credit_note_id")

  @@index([customer_balance_transaction_id], name: "credit_note_customer_balance_transaction_id_idx")
  @@index([customer_id], name: "credit_note_customer_id_idx")
  @@index([discount_amounts_discount_id], name: "credit_note_discount_amounts_discount_id_idx")
  @@index([invoice_id], name: "credit_note_invoice_id_idx")
  @@index([refund_id], name: "credit_note_refund_id_idx")
  @@index([tax_amounts_tax_rate_id], name: "credit_note_tax_amounts_tax_rate_id_idx")
  @@map("credit_note")
}

model credit_note_line_item {
  id                           String                     @id
  amount                       BigInt?
  description                  String?
  discount_amount              BigInt?
  discount_amounts_amount      BigInt?
  discount_amounts_discount_id String?
  invoice_line_item            String?
  livemode                     Boolean?
  object                       String?
  quantity                     BigInt?
  tax_amounts_amount           BigInt?
  tax_amounts_inclusive        Boolean?
  tax_amounts_tax_rate_id      String?
  type                         String?
  unit_amount                  BigInt?
  unit_amount_decimal          Decimal?                   @db.Decimal
  discount                     discount?                  @relation(fields: [discount_amounts_discount_id], references: [id])
  tax_rate                     tax_rate?                  @relation(fields: [tax_amounts_tax_rate_id], references: [id])
  credit_note_line_item_map    credit_note_line_item_map?

  @@index([discount_amounts_discount_id], name: "credit_note_line_item_discount_amounts_discount_id_idx")
  @@index([tax_amounts_tax_rate_id], name: "credit_note_line_item_tax_amounts_tax_rate_id_idx")
  @@map("credit_note_line_item")
}

model credit_note_line_item_map {
  credit_note_line_item_id String                @id
  credit_note_id           String?
  credit_note_line_item    credit_note_line_item @relation(fields: [credit_note_line_item_id], references: [id])

  @@index([credit_note_id], name: "credit_note_line_item_map_credit_note_id_idx")
  @@index([credit_note_line_item_id], name: "credit_note_line_item_map_credit_note_line_item_id_idx")
  @@map("credit_note_line_item_map")
}

model customer {
