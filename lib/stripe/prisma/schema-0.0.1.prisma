  level3_customer_reference                                          String?
  level3_line_items                                                  Json?
  level3_merchant_reference                                          String?
  level3_shipping_address_zip                                        String?
  level3_shipping_amount                                             BigInt?
  level3_shipping_from_zip                                           String?
