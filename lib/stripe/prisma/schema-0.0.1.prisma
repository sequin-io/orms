  currency                                                           String?
  customer_id                                                        String?
  description                                                        String?
  destination_id                                                     String?
  dispute_id                                                         String?
  disputed                                                           Boolean?
  failure_code                                                       String?
  failure_message                                                    String?
  fraud_details_stripe_report                                        String?
  fraud_details_user_report                                          String?
