model ConnectCollectionTransfer {
  id             String   @id
  amount         BigInt?
  currency       String?
  destination_id String?
  livemode       Boolean?
  object         String?
  account        Account? @relation(fields: [destination_id], references: [id])

  @@index([destination_id], name: "connect_collection_transfer_destination_id_idx")
  @@map("connect_collection_transfer")
}

model CountrySpec {
