  billing_address_collection                    String?
  cancel_url                                    String?
  client_reference_id                           String?
  currency                                      String?
  customer_details_email                        String?
  customer_details_tax_exempt                   String?
  customer_details_tax_ids                      Json?
  customer_email                                String?
  customer_id                                   String?
  livemode                                      Boolean?
  locale                                        String?
  metadata                                      Json?
  mode                                          String?
