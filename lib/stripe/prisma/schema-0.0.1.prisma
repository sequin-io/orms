  @@index([payment_intent_id], name: "charge_payment_intent_id_idx")
  @@index([review_id], name: "charge_review_id_idx")
  @@index([source_transfer_id], name: "charge_source_transfer_id_idx")
  @@index([transfer_data_destination_id], name: "charge_transfer_data_destination_id_idx")
  @@index([transfer_id], name: "charge_transfer_id_idx")
