generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id                                                       String                        @id
  business_profile_mcc                                     String?
  business_profile_name                                    String?
  business_profile_product_description                     String?
  business_profile_support_address                         Json?
  business_profile_support_email                           String?
  business_profile_support_phone                           String?
  business_profile_support_url                             String?
  business_profile_url                                     String?
  business_type                                            String?
  capabilities_acss_debit_payments                         String?
  capabilities_afterpay_clearpay_payments                  String?
  capabilities_au_becs_debit_payments                      String?
  capabilities_bacs_debit_payments                         String?
  capabilities_bancontact_payments                         String?
  capabilities_card_issuing                                String?
  capabilities_card_payments                               String?
  capabilities_cartes_bancaires_payments                   String?
  capabilities_eps_payments                                String?
  capabilities_fpx_payments                                String?
  capabilities_giropay_payments                            String?
  capabilities_grabpay_payments                            String?
  capabilities_ideal_payments                              String?
  capabilities_jcb_payments                                String?
  capabilities_legacy_payments                             String?
  capabilities_oxxo_payments                               String?
  capabilities_p24_payments                                String?
  capabilities_sepa_debit_payments                         String?
  capabilities_sofort_payments                             String?
  capabilities_tax_reporting_us_1099_k                     String?
  capabilities_tax_reporting_us_1099_misc                  String?
  capabilities_transfers                                   String?
  charges_enabled                                          Boolean?
  company_address                                          Json?
  company_address_kana                                     Json?
  company_address_kanji                                    Json?
  company_directors_provided                               Boolean?
  company_executives_provided                              Boolean?
  company_name                                             String?
  company_name_kana                                        String?
  company_name_kanji                                       String?
  company_owners_provided                                  Boolean?
  company_phone                                            String?
  company_structure                                        String?
  company_tax_id_provided                                  Boolean?
  company_tax_id_registrar                                 String?
  company_vat_id_provided                                  Boolean?
  company_verification                                     Json?
  country                                                  String?
  created                                                  DateTime?                     @db.Timestamp(6)
  default_currency                                         String?
  deleted                                                  Boolean?
  details_submitted                                        Boolean?
  email                                                    String?
  individual_id                                            String?
  metadata                                                 Json?
  object                                                   String?
  payouts_enabled                                          Boolean?
  requirements_current_deadline                            DateTime?                     @db.Timestamp(6)
  requirements_currently_due                               String[]
  requirements_disabled_reason                             String?
  requirements_errors                                      Json?
  requirements_eventually_due                              String[]
  requirements_past_due                                    String[]
  requirements_pending_verification                        String[]
  settings_bacs_debit_payments                             Json?
  settings_branding                                        Json?
  settings_card_issuing                                    Json?
  settings_card_payments                                   Json?
  settings_dashboard                                       Json?
  settings_payments                                        Json?
  settings_payouts                                         Json?
  settings_sepa_debit_payments                             Json?
  tos_acceptance_date                                      DateTime?                     @db.Timestamp(6)
  tos_acceptance_ip                                        String?
  tos_acceptance_service_agreement                         String?
  tos_acceptance_user_agent                                String?
  type                                                     String?
  individual                                               Person?                       @relation("account_individual_idToperson", fields: [individual_id], references: [id])
  application_fees                                         ApplicationFee[]
  bank_accounts                                            BankAccount[]
  capabilities                                             Capability[]
  cards                                                    Card[]
  destination_charges                                      Charge[]                      @relation("accountTocharge_destination_id")
  on_behalf_of_charges                                     Charge[]                      @relation("accountTocharge_on_behalf_of_id")
  transfer_data_destination_charges                        Charge[]                      @relation("accountTocharge_transfer_data_destination_id")
  connect_collection_transfers                             ConnectCollectionTransfer[]
  events                                                   Event[]
  on_behalf_of_invoices                                    Invoice[]                     @relation("accountToinvoice_on_behalf_of_id")
  transfer_data_destination_invoices                       Invoice[]                     @relation("accountToinvoice_transfer_data_destination_id")
  on_behalf_of_payment_intents                             PaymentIntent[]               @relation("accountTopayment_intent_on_behalf_of_id")
  transfer_data_destination_payment_intents                PaymentIntent[]               @relation("accountTopayment_intent_transfer_data_destination_id")
  persons                                                  Person[]                      @relation("accountToperson_account_id")
  platform_tax_fees                                        PlatformTaxFee[]
  migrated_to_recipients                                   Recipient[]                   @relation("accountTorecipient_migrated_to_id")
  rolled_back_from_recipients                              Recipient[]                   @relation("accountTorecipient_rolled_back_from_id")
  setup_attempts                                           SetupAttempt[]
  setup_intents                                            SetupIntent[]
  subscriptions                                            Subscription[]
  transfers                                                Transfer[]
  on_behalf_of_upcoming_customer_invoices                  UpcomingCustomerInvoice[]     @relation("accountToupcoming_customer_invoice_on_behalf_of_id")
  transfer_data_destination_upcoming_customer_invoices     UpcomingCustomerInvoice[]     @relation("accountToupcoming_customer_invoice_transfer_data_destination_id")
  on_behalf_of_upcoming_subscription_invoices              UpcomingSubscriptionInvoice[] @relation("accountToupcoming_subscription_invoice_on_behalf_of_id")
  transfer_data_destination_upcoming_subscription_invoices UpcomingSubscriptionInvoice[] @relation("accountToupcoming_subscription_invoice_transfer_data_destination_id")

  @@index([individual_id], name: "account_individual_id_idx")
  @@map("account")
}

model AlipayAccount {
  id               String    @id
  created          DateTime? @db.Timestamp(6)
  customer_id      String?
  deleted          Boolean?
  fingerprint      String?
  livemode         Boolean?
  metadata         Json?
  object           String?
  payment_amount   BigInt?
  payment_currency String?
  reusable         Boolean?
  used             Boolean?
  username         String?
  customer         Customer? @relation(fields: [customer_id], references: [id])

  @@index([customer_id], name: "alipay_account_customer_id_idx")
  @@map("alipay_account")
}

model Application {
  id                            String                       @id
  name                          String?
  object                        String?
  application_fees              ApplicationFee[]
  billing_portal_configurations BillingPortalConfiguration[]
  charges                       Charge[]
  orders                        Order[]
  payment_intents               PaymentIntent[]
  setup_attempts                SetupAttempt[]
  setup_intents                 SetupIntent[]
  webhook_endpoints             WebhookEndpoint[]

  @@map("application")
}

model ApplicationFee {
  id                         String              @id
  account_id                 String?
  amount                     BigInt?
  amount_refunded            BigInt?
  application_id             String?
  balance_transaction_id     String?
  charge_id                  String?
  created                    DateTime?           @db.Timestamp(6)
  currency                   String?
  livemode                   Boolean?
  object                     String?
  originating_transaction_id String?
  refunded                   Boolean?
  account                    Account?            @relation(fields: [account_id], references: [id])
  application                Application?        @relation(fields: [application_id], references: [id])
  balance_transaction        BalanceTransaction? @relation(fields: [balance_transaction_id], references: [id])
  charge                     Charge?             @relation("application_fee_charge_idTocharge", fields: [charge_id], references: [id])
  originating_transaction    Charge?             @relation("application_fee_originating_transaction_idTocharge", fields: [originating_transaction_id], references: [id])
  charges                    Charge[]            @relation("application_feeTocharge_application_fee_id")
  fee_refunds                FeeRefund[]

  @@index([account_id], name: "application_fee_account_id_idx")
  @@index([application_id], name: "application_fee_application_id_idx")
  @@index([balance_transaction_id], name: "application_fee_balance_transaction_id_idx")
  @@index([charge_id], name: "application_fee_charge_id_idx")
  @@index([originating_transaction_id], name: "application_fee_originating_transaction_id_idx")
  @@map("application_fee")
}

model BalanceTransaction {
  id                      String               @id
  amount                  BigInt?
  available_on            DateTime?            @db.Timestamp(6)
  created                 DateTime?            @db.Timestamp(6)
  currency                String?
  description             String?
  exchange_rate           Decimal?             @db.Decimal
  fee                     BigInt?
  fee_details_amount      BigInt?
  fee_details_application String?
  fee_details_currency    String?
  fee_details_description String?
  fee_details_type        String?
  net                     BigInt?
  object                  String?
  reporting_category      String?
  status                  String?
  type                    String?
  application_fees        ApplicationFee[]
  charges                 Charge[]
  fee_refunds             FeeRefund[]
  issuing_transactions    IssuingTransaction[]
  payouts                 Payout[]             @relation("balance_transactionTopayout_balance_transaction_id")
  failure_payouts         Payout[]             @relation("balance_transactionTopayout_failure_balance_transaction_id")
  refunds                 Refund[]             @relation("balance_transactionTorefund_balance_transaction_id")
  failure_refunds         Refund[]             @relation("balance_transactionTorefund_failure_balance_transaction_id")
  topups                  Topup[]
  transfers               Transfer[]
  transfer_reversals      TransferReversal[]

  @@map("balance_transaction")
}

model BankAccount {
  id                       String      @id
  account_holder_name      String?
  account_holder_type      String?
  account_id               String?
  available_payout_methods String[]
  bank_name                String?
  country                  String?
  currency                 String?
  customer_id              String?
  default_for_currency     Boolean?
  deleted                  Boolean?
  fingerprint              String?
  last4                    String?
  metadata                 Json?
  object                   String?
  routing_number           String?
  status                   String?
  account                  Account?    @relation(fields: [account_id], references: [id])
  customer                 Customer?   @relation(fields: [customer_id], references: [id])
  recipients               Recipient[]
  tokens                   Token[]

  @@index([account_id], name: "bank_account_account_id_idx")
  @@index([customer_id], name: "bank_account_customer_id_idx")
  @@map("bank_account")
}

model BillingPortalConfiguration {
  id                                    String                 @id
  active                                Boolean?
  application_id                        String?
  business_profile_headline             String?
  business_profile_privacy_policy_url   String?
  business_profile_terms_of_service_url String?
  created                               DateTime?              @db.Timestamp(6)
  default_return_url                    String?
  features_customer_update              Json?
  features_invoice_history              Json?
  features_payment_method_update        Json?
  features_subscription_cancel          Json?
  features_subscription_pause           Json?
  features_subscription_update          Json?
  is_default                            Boolean?
  livemode                              Boolean?
  object                                String?
  updated                               DateTime?              @db.Timestamp(6)
  application                           Application?           @relation(fields: [application_id], references: [id])
  billing_portal_sessions               BillingPortalSession[]

  @@map("billing_portal_configuration")
}

model BillingPortalSession {
  id                           String                      @id
  configuration_id             String?
  created                      DateTime?                   @db.Timestamp(6)
  customer_id                  String?
  livemode                     Boolean?
  object                       String?
  on_behalf_of                 String?
  return_url                   String?
  url                          String?
  billing_portal_configuration BillingPortalConfiguration? @relation(fields: [configuration_id], references: [id])
  customer                     Customer?                   @relation(fields: [customer_id], references: [id])

  @@index([configuration_id], name: "billing_portal_session_configuration_id_idx")
  @@map("billing_portal_session")
}

model BitcoinReceiver {
  id                      String               @id
  active                  Boolean?
  amount                  BigInt?
  amount_received         BigInt?
  bitcoin_amount          BigInt?
  bitcoin_amount_received BigInt?
  bitcoin_uri             String?
  created                 DateTime?            @db.Timestamp(6)
  currency                String?
  customer_id             String?
  deleted                 Boolean?
  description             String?
  email                   String?
  filled                  Boolean?
  inbound_address         String?
  livemode                Boolean?
  metadata                Json?
  object                  String?
  payment                 String?
  refund_address          String?
  uncaptured_funds        Boolean?
  used_for_payment        Boolean?
  customer                Customer?            @relation(fields: [customer_id], references: [id])
  bitcoin_transactions    BitcoinTransaction[]

  @@map("bitcoin_receiver")
}

model BitcoinTransaction {
  id               String           @id
  amount           BigInt?
  bitcoin_amount   BigInt?
  created          DateTime?        @db.Timestamp(6)
  currency         String?
  object           String?
  receiver         String?
  bitcoin_receiver BitcoinReceiver? @relation(fields: [receiver], references: [id])

  @@map("bitcoin_transaction")
}

model Capability {
  id                                String    @id
  account_id                        String?
  object                            String?
  requested                         Boolean?
  requested_at                      DateTime? @db.Timestamp(6)
  requirements_current_deadline     DateTime? @db.Timestamp(6)
  requirements_currently_due        String[]
  requirements_disabled_reason      String?
  requirements_errors               Json?
  requirements_eventually_due       String[]
  requirements_past_due             String[]
  requirements_pending_verification String[]
  status                            String?
  account                           Account?  @relation(fields: [account_id], references: [id])

  @@index([account_id], name: "capability_account_id_idx")
  @@map("capability")
}

model Card {
  id                       String         @id
  account_id               String?
  address_city             String?
  address_country          String?
  address_line1            String?
  address_line1_check      String?
  address_line2            String?
  address_state            String?
  address_zip              String?
  address_zip_check        String?
  available_payout_methods String[]
  brand                    String?
  country                  String?
  currency                 String?
  customer_id              String?
  cvc_check                String?
  default_for_currency     Boolean?
  deleted                  Boolean?
  description              String?
  dynamic_last4            String?
  exp_month                BigInt?
  exp_year                 BigInt?
  fingerprint              String?
  funding                  String?
  iin                      String?
  issuer                   String?
  last4                    String?
  metadata                 Json?
  name                     String?
  object                   String?
  recipient_id             String?
  tokenization_method      String?
  account                  Account?       @relation(fields: [account_id], references: [id])
  customer                 Customer?      @relation(fields: [customer_id], references: [id])
  recipient                Recipient?     @relation("card_recipient_idTorecipient", fields: [recipient_id], references: [id])
